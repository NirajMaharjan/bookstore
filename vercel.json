{
    "functions": {
      "index.php": {
        "runtime": "vercel-php@0.5.0"
      },
      "pages/*.php": {
        "runtime": "vercel-php@0.5.0"
      }
    },
    "routes": [
      { "src": "/assets/(.*)", "dest": "/assets/$1" },
      { "src": "/pages/(.*)", "dest": "/pages/$1" },
      { "src": "/(.*)", "dest": "/index.php" }
    ]
  }
  